@model IEnumerable<ClinicApp.Models.SpecialityModel>

@{
    ViewBag.Title = "AddReceptionSpecialities";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ul class="nav nav-tabs" style="margin-top: 50px;">
    <li>@Html.ActionLink("Главная", "Index", "Patient")</li>
    <li>@Html.ActionLink("Диагнозы", "Records", "Patient")</li>
    <li class="active">@Html.ActionLink("Приёмы", "Receptions", "Patient")</li>
</ul>


<ul class="list-group" style="margin-top: 50px">
    @foreach (var item in Model)
    {
        <li class="list-group-item">
            @Html.ActionLink(item.Name, "AddReceptionDoctor", "Patient", new { specId = item.Id }, null)
        </li>
    }
</ul>